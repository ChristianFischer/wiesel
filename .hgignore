syntax: glob

# Mercurial local files
*.reject
*.rej
*.orig

# OS specific excludes
.DS_Store
.directory
Thumbs.db

# Android build files & local settings (including android projects in nested folders)
regexp:android(/.+)*/gen/
regexp:android(/.+)*/libs/.*/
regexp:android(/.+)*/bin/
regexp:android(/.+)*/obj/
regexp:android(/.+)*/assets/
regexp:android(/.+)*/local.properties

# Any build folders (excluding the build-directory in the repository root)
*/build/*

# Local config folders
config/local/

# Eclipse project files
.classpath
.project
.cproject
.settings/

